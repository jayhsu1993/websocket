.module .content.game-money {
    background-color: #ffe8ca;
    overflow: hidden;
}

.money-rank {
    height: 330px;
    width: 100%;
    position: absolute;
    bottom: 0;
}

.money-rank .warp {
    position: relative;
    bottom: 80px;
    z-index: 800;
    margin-left: 320px;
}

.money-rank .warp .rank-itema {
    margin: 0;
    padding: 0;
    list-style: none;
    width: 128px;
    height: 300px;
    float: left;
    position: relative;
}

.money-rank .warp .rank-itema .rank-item-b {
    position: absolute;
    bottom: 0;
    width: 100%;
}

.money-rank .warp .rank-itema .rank-item-b .username {
    display: block;
    color: #000;
    text-align: center;
}

.money-rank .warp .rank-itema .rank-item-b .userpoint {
    display: block;
    color: #000;
    text-align: center;
}

.money-rank .warp .rank-itema .rank-item-b .touxiang {
    margin-bottom: -10px;
    position: relative;
    text-align: center;
}


/* .money-rank .warp .rank-item .rank-item-b .touxiang .kuang {} */

.money-rank .warp .rank-itema .rank-item-b .touxiang .user {
    position: absolute;a
    top: 2px;
    left: 40px;
    height: 48px;
    width: 48px;
    border-radius: 50px;
}

.money-rank .warp .rank-itema .rank-item-b .paiming-b {
    margin-top: -10px;
    position: relative;
    text-align: center;
}

.money-rank .warp .rank-itema .rank-item-b .paiming-b .mingci {
    position: absolute;
    color: #FFF;
    top: 1px;
    left: 59px;
}

.money-rank .warp .rank-itema .rank-item-b .paiming-b .mingci.t {
    left: 51px
}

.money-rank .warp .rank-itema .rank-item-b .progress-bar {
    width: 40px;
    height: 10px;
    border: 3px solid #940;
    margin: 0 auto;
    background-color: #fee121;
    background-image: url(../img/game/qwed_03.png);
    background-position-x: -3px;
    animation: myprogress 4s infinite linear;
}

@keyframes myprogress {
    from {
        background-position-y: 200px
    }
    to {
        background-position-y: 0
    }
}

@-webkit-keyframes myprogress {
    from {
        background-position-y: 200px
    }
    to {
        background-position-y: 0
    }
}

.money-stage .sky {
    height: 271px;
    width: auto;
    position: absolute;
    top: 0;
    right: 0;
    /* background-image: url(../img/money/SKY.png);
    background-repeat: no-repeat; */
}

.money-stage .ground {
    height: 318px;
    width: auto;
    position: absolute;
    top: 226px;
    right: 0;
    /* background-image: url(../img/money/ground.png);
    background-repeat: no-repeat; */
}

.money-stage .ground.move {
    animation: money-move 15s linear infinite;
    -webkit-animation: money-move 15s linear infinite
}

.money-stage .sky.move {
    animation: money-move 20s linear infinite;
    -webkit-animation: money-move 20s linear infinite
}

@keyframes money-move {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(3840px)
    }
}

@-webkit-keyframes money-move {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(3840px)
    }
}

.money-stage .man {
    position: absolute;
    /* overflow: hidden; */
    top: 200px;
    left: 1400px;
}

.money-stage .man .p img.move {
    animation: money-man-run 1.5s steps(17) infinite;
    -webkit-animation: money-man-run 1.5s steps(17) infinite
}

.money-stage .man .p img.move.end {
    animation: money-man-run-end 1.5s steps(5) infinite;
    -webkit-animation: money-man-run-end 1.5s steps(5) infinite
}

.money-stage .man .l img.move {
    animation: money-fhl 1s linear infinite;
    -webkit-animation: money-fhl 1s linear infinite
}

.money-stage .man .p {
    width: 222px;
    height: 280px;
    margin: 0 auto;
    overflow: hidden;
}

.money-stage .man .l {
    margin-top: -30px;
    width: 250px;
    height: 250px;
}

.money-stage .man .l img {
    height: 100%;
    width: 100%;
}

.money-stage .goldbox {
    position: absolute;
    top: 470px;
    left: -293px;
}

.money-stage .goldbox.move {
    animation: money-goldbox 3s linear forwards;
    -webkit-animation: money-goldbox 3s linear forwards;
    	animation-delay: 26s;
	-webkit-animation-delay: 26s;
}

.money-stage .facai {
    position: absolute;
    left: 290px;
    top: 180px;
    transform: scaleY(0)
}

.money-stage .facai.move {
    animation: money-facai 1s linear forwards;
    -webkit-animation: money-facai 1s linear forwards;
    	animation-delay: 29s;
	-webkit-animation-delay: 29s;
}

.money-stage .man.move {
    animation: money-warp-move 30s linear forwards;
    -webkit-animation: money-warp-move 30s linear forwards
}

.money-stage .sun {
    position: absolute;
    left: 500px;
    top: 100px;
}

.money-stage .sun.move {
    animation: money-sun 2s linear infinite;
    -webkit-animation: money-sun 2s linear infinite
}

.money-stage .ship {
    position: absolute;
    left: 1700px;
    top: 184px;
}

.money-stage .ship.move {
    animation: money-man-run 60s linear infinite;
    -webkit-animation: money-man-run 60s linear infinite
}

.money-stage .s {
    position: absolute;
    height: 270px;
    width: 270px;
    top: 255px;
    left: 13px;
}

.money-stage .s img {
    height: 250px;
    width: 250px;
}

.money-stage .ds {
    position: absolute;
    top: 150px;
    width:115px;
    text-align:center;
    left: 850px;
    font-size: 80px;
    color: #fff;
    font-family: Impact;
    padding: 20px;
 text-shadow: 3px 3px 3px rgba(0, 0, 0, .8);
 display:none;
}

@keyframes money-facai {
    from {
        transform: scaleY(0)
    }
    to {
        transform: scaleY(1)
    }
}

@keyframes money-goldbox {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(500px)
    }
}

@keyframes money-man-run-end {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(-1110px)
    }
}

@keyframes money-man-run {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(-3774px)
    }
}

@keyframes money-fhl {
    from {
        transform: rotate(360deg)
    }
    to {
        transform: rotate(0);
    }
}

@keyframes money-warp-move {
    from {
        transform: translateX(0)
    }
    to {
        transform: translateX(-900px)
    }
}

@keyframes money-sun {
    0% {
        transform: rotate(0)
    }
    25% {
        transform: rotate(20deg)
    }
    75% {
        transform: rotate(-20deg)
    }
    100% {
        transform: rotate(0)
    }
}