.sup {
	animation: suspension 2s ease infinite;
	-webkit-animation: suspension 2s ease infinite
}


/*震动*/

@-webkit-keyframes suspension {
	0% {
		-webkit-transform: translateY(2px) perspective(500px) rotateY(15deg);
	}
	50% {
		-webkit-transform: translateY(4px) perspective(500px) rotateY(15deg);
	}
	100% {
		-webkit-transform: translateY(2px) perspective(500px) rotateY(15deg);
	}
}


/*震动*/

@keyframes suspension {
	0% {
		transform: translateY(2px) perspective(500px) rotateY(15deg);
	}
	50% {
		transform: translateY(4px) perspective(500px) rotateY(15deg);
	}
	100% {
		transform: translateY(2px) perspective(500px) rotateY(15deg);
	}
}

@keyframes zuobiao {
	0% {
		transform: translateY(2px) perspective(500px) scale(1.1);
	}
	50% {
		transform: translateY(4px) perspective(500px)  scale(1.0);
	}
	100% {
		transform: translateY(2px) perspective(500px) scale(1.1);
	}
}

@-webkit-keyframes zuobiao {
	0% {
		transform: translateY(2px) perspective(500px) scale(1.1);
	}
	50% {
		transform: translateY(4px) perspective(500px)  scale(1.0);
	}
	100% {
		transform: translateY(2px) perspective(500px)  scale(1.1);
	}
}

.disabled {
	pointer-events: none;
}