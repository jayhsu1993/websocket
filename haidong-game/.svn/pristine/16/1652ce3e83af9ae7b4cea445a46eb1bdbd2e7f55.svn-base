/*闇囧姩*/
@-webkit-keyframes suspension {
    0% {
        -webkit-transform: translateY(2px) perspective(500px) rotateY(15deg);
    }
    
    50% {
        -webkit-transform: translateY(4px) perspective(500px) rotateY(15deg);
    }
    
    100% {
        -webkit-transform: translateY(2px) perspective(500px) rotateY(15deg);
    }
}

/*闇囧姩*/
