    0% {
        transform: perspective(600px) rotateY(0)
    }
    10%,
    to {
        transform: perspective(600px) rotateY(360deg)
    }
}

@keyframes rotate {
    0% {
        opacity: 0
    }
    0%,
    30% {
        transform: perspective(600px) rotateY(0)
    }
    30%,
    70%,
    to {
        opacity: 1
    }
    70%,
    to {
        transform: perspective(600px) rotateY(360deg)
    }
}

@-webkit-keyframes rotate {
    0% {
        opacity: 0
    }
    0%,
    30% {
        -webkit-transform: perspective(600px) rotateY(0)
    }
    30%,
    70%,
    to {
        opacity: 1
    }
    70%,
    to {
        -webkit-transform: perspective(600px) rotateY(360deg)
    }
}

@keyframes animation {
    0% {
        opacity: 0;
        transform: translate(0, -300px) perspective(600px) rotateY(0)
    }
    30% {
        opacity: .7;
        transform: translate(0, 0) perspective(600px) rotateY(0)
    }
    70%,
    to {
        opacity: .7;
        transform: translate(0, 0) perspective(600px) rotateY(360deg)
    }
}

@-webkit-keyframes animation {
    0% {
        opacity: 0;
        -webkit-transform: translate(0, -300px) perspective(600px) rotateY(0)
    }
    30% {
        opacity: .7;
